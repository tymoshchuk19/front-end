members/',friend.email)\" \n            >\n                <v-icon size=\"20\">mdi-check-circle</v-icon>\n          </v-btn>\n      </v-card>\n    </v-expansion-panel>\n  </v-expansion-panels>\n</template>\n<script>\nimport { API } from \"../../config/config\";\nimport axios from \"axios\";\n/*eslint-disable no-console*/\n  export default {\n    props: ['friends','perms','group'],\n    data () {\n      return {\n      }\n    },\n    methods: {\n      deleteMember(path) {\n        axios\n          .delete(API + \"groups/\" + path)\n          .then(() => \n            this.$emit('newMember')\n          );\n      },\n      addMember(path, email) {\n        axios\n          .post(API + \"groups/\" + path,{\n              members: [email]\n          })\n          .then(() => \n            this.$emit('newMember')\n          );\n      }\n    }\n  }\n</script>"]}]}