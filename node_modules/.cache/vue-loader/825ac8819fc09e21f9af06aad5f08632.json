ntent>\n        {{ user.bDate }}\n      </v-list-item-content>\n    </v-list-item>\n    <div class=\"pa-2 mb--1\">\n      <v-btn\n        v-if=\"!mates\"\n        color=\"indigo darken-1\"\n        class=\"text--white\"\n        @click=\"addFriend\"\n        >Add friend</v-btn\n      >\n      <v-btn\n        v-else\n        color=\"indigo darken-1\"\n        class=\"text--white\"\n        @click=\"removeFriend\"\n        >Remove friend</v-btn\n      >\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport { API } from \"../../config/config\";\n\nimport axios from \"axios\";\nexport default {\n  props: [\"user\"],\n  data() {\n    return {\n      mates: false\n    };\n  },\n  methods: {\n    addFriend() {\n      axios\n        .post(API + \"users/\" + this.user._id + \"/request\", {})\n        .then(() => {});\n    },\n    removeFriend() {\n      axios.post(API + \"users/\" + this.user._id + \"/remove\", {}).then(() => {\n        this.mates = !this.mates;\n      });\n    }\n  },\n  mounted() {\n    this.mates = this.state.user.friends.find(obj => {\n      return obj._id === this.$route.params.user_id;\n    });\n  }\n};\n</script>\n"]}]}