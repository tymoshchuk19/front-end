xios\n        .get(API + \"users/\" + this.user._id + \"/tasks\")\n        .then(data => (this.tasks = data.data));\n    }\n  },\n  mounted() {\n    this.getPosts();\n    this.getGroups();\n    this.getTasks();\n  },\n  computed: {\n    ...mapState([\"user\"])\n  }\n};\n</script>\n"]}]}