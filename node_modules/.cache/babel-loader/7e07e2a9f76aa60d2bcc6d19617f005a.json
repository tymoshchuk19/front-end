,KAAA;AASA,GAxBA;AAyBA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA,gBAAA,EAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AAEA,WAAA,QAAA,CAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA,WAAA,SAAA;AACA,WAAA,QAAA,CAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA,WAAA,QAAA,CAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA;AARA,GAzBA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,KAAA,OAAA,EAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AAAA,OAAA;AACA,KAHA;AAIA,IAAA,QAJA,oBAIA,QAJA,EAIA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,CACA,GAAA,GAAA,SAAA,GAAA,QADA,EAEA,IAFA,CAEA,UAAA,IAAA;AAAA,eAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AAAA,OAFA;AAGA,KARA;AASA,IAAA,SATA,uBASA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,QAAA,EAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,MAAA,CAAA,MAAA,GAAA,IAAA,CAAA,IAAA;AAAA,OAAA;AACA,KAXA;AAYA,IAAA,QAZA,sBAYA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,KAAA,OAAA,EAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AAAA,OAAA;AACA;AAdA,GAnCA;AAmDA,EAAA,OAnDA,qBAmDA;AACA,SAAA,QAAA;AACA,SAAA,SAAA;AACA,SAAA,QAAA;AACA,SAAA,QAAA;AACA;AAxDA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-row>\n      <v-col cols=\"1\">\n        <wm-sidebar></wm-sidebar>\n      </v-col>\n      <v-col cols=\"11\">\n        <v-row>\n          <v-col cols=\"2\">\n            <wm-workum></wm-workum>\n          </v-col>\n          <v-col cols=\"6\">\n            <wm-header :name=\"group.name\"></wm-header>\n          </v-col>\n          <v-col cols=\"3\">\n            <wm-signout></wm-signout>\n          </v-col>\n        </v-row>\n        <v-row>\n          <v-col cols=\"2\" class=\"secondary\">\n            <wm-groupform @newGroup=\"getGroups\"></wm-groupform>\n            <wm-groups :groups=\"groups\" :path=\"path\"></wm-groups>\n          </v-col>\n          <v-col cols=\"6\">\n            <wm-postform :api=\"postApi\" @newPost=\"getPosts\"></wm-postform>\n            <v-divider class=\"mb-2\"></v-divider>\n            <wm-posts :posts=\"posts\"></wm-posts>\n          </v-col>\n          <v-col cols=\"3\" class=\"secondary\">\n            <wm-taskform @newTask=\"getTasks\" :taskApi=\"taskApi\"></wm-taskform>\n            <wm-tasks :tasks=\"tasks\"></wm-tasks>\n          </v-col>\n        </v-row>\n      </v-col>\n    </v-row>\n    <wm-footer></wm-footer>\n  </div>\n</template>\n\n<script>\n/*eslint-disable no-console*/\n\nimport Posts from \"../components/Posts\";\nimport PostForm from \"../components/PostForm\";\nimport Workum from \"../components/Workum\";\nimport Header from \"../components/Header\";\nimport Signout from \"../components/Signout\";\nimport Tasks from \"../components/Tasks\";\nimport TasksForm from \"../components/TasksForm\";\nimport GroupForm from \"../components/GroupForm\";\nimport Groups from \"../components/Groups\";\nimport { API } from \"../../config/config\";\nimport axios from \"axios\";\nimport Footer from \"../components/Footer\";\nimport SideBar from \"../components/SideBar\";\n\nexport default {\n  components: {\n    \"wm-posts\": Posts,\n    \"wm-postform\": PostForm,\n    \"wm-workum\": Workum,\n    \"wm-header\": Header,\n    \"wm-signout\": Signout,\n    \"wm-tasks\": Tasks,\n    \"wm-taskform\": TasksForm,\n    \"wm-groupform\": GroupForm,\n    \"wm-groups\": Groups,\n    \"wm-footer\": Footer,\n    \"wm-sidebar\": SideBar\n  },\n  data() {\n    return {\n      path: \"\",\n      group: { name: null },\n      posts: null,\n      groups: null,\n      tasks: null,\n      postApi: API + \"groups/\" + this.$route.params.group_id + \"/posts\",\n      taskApi: API + \"groups/\" + this.$route.params.group_id + \"/tasks\"\n    };\n  },\n  watch: {\n    $route: function(to) {\n      console.log(to.params.group_id);\n\n      this.getGroup(to.params.group_id);\n      this.getGroups();\n      this.getPosts(to.params.group_id);\n      this.getTasks(to.params.group_id);\n    }\n  },\n  methods: {\n    getPosts() {\n      axios.get(this.postApi).then(data => (this.posts = data.data.posts));\n    },\n    getGroup(group_id) {\n      axios\n        .get(API + \"groups/\" + group_id)\n        .then(data => (this.group = data.data));\n    },\n    getGroups() {\n      axios.get(API + \"groups\").then(data => (this.groups = data.data));\n    },\n    getTasks() {\n      axios.get(this.taskApi).then(data => (this.tasks = data.data));\n    }\n  },\n  mounted() {\n    this.getGroup();\n    this.getGroups();\n    this.getPosts();\n    this.getTasks();\n  }\n};\n</script>"],"sourceRoot":"src/views"}]}